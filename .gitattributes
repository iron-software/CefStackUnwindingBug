# Auto detect text files and perform LF normalization
* text=auto
*.so filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
LibCefWebLinuxShort/cef_my/**/* filter=lfs diff=lfs merge=lfs -text
LibCefWebLinuxShort/cef_my_release/**/* filter=lfs diff=lfs merge=lfs -text
*.a filter=lfs diff=lfs merge=lfs -text
*.pak filter=lfs diff=lfs merge=lfs -text
*.so.1 filter=lfs diff=lfs merge=lfs -text
chrome-sandbox filter=lfs diff=lfs merge=lfs -text
*.dat filter=lfs diff=lfs merge=lfs -text
